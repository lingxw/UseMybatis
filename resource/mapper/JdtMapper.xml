<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="JdtDao">
	<insert
	  id="insertBinding"
	  parameterType="JdtBinding"
	  keyColumn="id"
	  keyProperty="id"
	  useGeneratedKeys="true">
	  insert into jtdBinding 
	  (path, name, [key], srcId)
	  values (#{path}, #{name}, #{key}, #{srcId})
	</insert>
</mapper>